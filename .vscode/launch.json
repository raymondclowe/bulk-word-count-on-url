    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: example.txt with tags",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"  ,
             "args": [
                "example.txt",
                "--tags",
                "p"
            ]
        },
        {
            "name": "Python: example url",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "http://example.com"
            ]
        }
    ]
}